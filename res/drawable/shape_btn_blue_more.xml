<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@android:color/white" android:endColor="#ffcccccc" android:angle="180.0" />
            <stroke android:width="1.0px" android:color="#5b5bfe" />
            <solid android:color="#dcdcdc" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" android:bottomLeftRadius="3.0dip" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@android:color/white" android:endColor="#ffcccccc" android:angle="180.0" />
            <stroke android:width="1.0px" android:color="#5b5bfe" />
            <solid android:color="@android:color/white" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" android:bottomLeftRadius="3.0dip" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
    <item android:state_selected="true">
        <shape android:shape="rectangle">
            <gradient android:startColor="@android:color/white" android:endColor="#ffcccccc" android:angle="180.0" />
            <stroke android:width="1.0px" android:color="#5b5bfe" />
            <solid android:color="#dcdcdc" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" android:bottomLeftRadius="3.0dip" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <gradient android:startColor="#dcdcdc" android:endColor="#dcdcdc" android:angle="180.0" />
            <stroke android:width="1.0px" android:color="#dcdcdc" />
            <solid android:color="#dcdcdc" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" android:bottomLeftRadius="3.0dip" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@android:color/white" android:endColor="#ffcccccc" android:angle="180.0" />
            <stroke android:width="1.0px" android:color="#5b5bfe" />
            <solid android:color="@android:color/white" />
            <corners android:topLeftRadius="3.0dip" android:topRightRadius="3.0dip" android:bottomLeftRadius="3.0dip" android:bottomRightRadius="3.0dip" />
        </shape>
    </item>
</selector>